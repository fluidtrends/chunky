{
  "name": "react-electron-chunky",
  "productName": "chunky",
  "version": "1.1.2",
  "preferGlobal": true,
  "description": "The Chunky Desktop framework",
  "main": ".webpack/main",
  "scripts": {
    "start": "node bin/start",
    "package": "node bin/package",
    "make": "electron-forge make",
    "rebuild": "electron-rebuild -f"
  },
  "keywords": [],
  "author": {
    "name": "I. Dan Calinescu",
    "email": "idancalinescu@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "assets/logo.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "carmel"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "name": "carmel",
            "icon": "assets/logo.icns"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./ui/index.html",
                  "js": "./ui/index.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "antd": "^3.23.3",
    "copyfiles": "^2.1.1",
    "cpy": "^8.0.0",
    "decompress": "^4.2.0",
    "download": "^7.1.0",
    "electron-dl": "^1.14.0",
    "electron-log": "^4.0.0",
    "electron-main-fetch": "^0.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "favicon-getter": "^1.1.3",
    "fs-extra": "^8.1.0",
    "js-base64": "^2.5.1",
    "jsonfile": "^5.0.0",
    "lali": "^1.0.0",
    "node-7z": "^1.1.1",
    "opn": "^6.0.0",
    "os": "^0.1.1",
    "os-name": "^3.1.0",
    "react": "^16.9.0",
    "react-detect-offline": "^2.4.0",
    "react-dom": "^16.9.0",
    "react-markdown": "^4.2.2",
    "react-reveal": "^1.2.2",
    "react-spring": "^8.0.27",
    "recursive-readdir": "^2.2.2",
    "rsync": "^0.6.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "url-loader": "^2.2.0",
    "uuid": "^3.3.3",
    "@electron-forge/cli": "6.0.0-beta.45",
    "@electron-forge/maker-deb": "6.0.0-beta.45",
    "@electron-forge/maker-dmg": "6.0.0-beta.45",
    "@electron-forge/maker-rpm": "6.0.0-beta.45",
    "@electron-forge/maker-squirrel": "6.0.0-beta.45",
    "@electron-forge/maker-zip": "6.0.0-beta.45",
    "@electron-forge/plugin-webpack": "6.0.0-beta.45"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.1",
    "css-loader": "^3.0.0",
    "electron": "^6.0.11",
    "electron-rebuild": "^1.8.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "node-loader": "^0.6.0",
    "prop-types": "^15.7.2",
    "style-loader": "^0.23.1"
  }
}
