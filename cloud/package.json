{
  "name": "react-cloud-chunky",
  "version": "1.0.0",
  "description": "The Chunky Cloud Framework",
  "main": "lib/index.js",
  "scripts": {
    "transpile": "node_modules/babel-cli/bin/babel.js src --out-dir lib --plugins=styled-jsx/babel",
    "test": "react-savor test",
    "lint": "react-savor lint",
    "coverage": "react-savor coverage",
    "codeclimate": "react-savor codeclimate",
    "docs": "react-savor docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fluidtrends/chunky.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fluidtrends/chunky/issues"
  },
  "homepage": "https://github.com/fluidtrends/chunky",
  "babel": {
    "plugins": [
      "transform-react-jsx",
      "transform-es2015-destructuring",
      "transform-object-rest-spread",
      "css-modules-transform",
      "styled-jsx/babel"
    ],
    "presets": [
      "react",
      "env"
    ]
  },
  "dependencies": {
    "firebase-admin": "^5.1.0",
    "firebaseline": "0.x",
    "js-base64": "^2.4.3",
    "node-fetch": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-css-modules-transform": "^1.5.0",
    "babel-plugin-react-css-modules": "^3.2.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1"
  }
}
