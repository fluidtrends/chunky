{
  "name": "chunky",
  "env": "development",
  "id": "io.chunky",
  "androidSdkDir": "~/Library/Android/sdk",
  "app": {
    "start": {
      "stack": ["intro", "docs"]
    }
  },
  "transitions": ["replace://start"],
  "provisioning": {},
  "theme": {
    "logoImage": "logo.gif",
    "logoLightImage": "logo.gif",
    "statusBarLight": false,
    "titleTextSize": 50,
    "subtitleTextSize": 32,
    "headingTextSize": 24,  
    "subheadingTextSize": 22,  
    "paragraphTextSize": 20,
    "codeTextSize": 14,
    "headerColor": "#FF5722",
    "textColor": "#546E7A",
    "linkColor": "#0288D1",
    "hoverColor": "#1B5E20",
    "progressColor": "rgba(50,50,50,0.9)",
    "primaryColor": "#2196F3",
    "secondaryColor": "#4CAF50",
    "navigationColor": "#FFFFFF",
    "navigationTintColor": "#37474F",
    "backgroundColor": "#999999",
    "footerTintColor": "#CFD8DC",
    "footerHeaderColor": "#90A4AE",
    "footerColor": "#546E7A",
    "footerBottomColor": "#37474F",
    "footerSocialIconsColor": "#fff",
    "footerSocialIconsColorHover": "#37474F"
  },
  "info": {
    "copyright": "2020",
    "watermark": "Created with Chunky"
  },
  "template": "personal",
  "styles": {
    "web": [
      "https://fonts.googleapis.com/css?family=Roboto",
      "https://fonts.googleapis.com/css?family=Montserrat:400,700",
      "https://fonts.googleapis.com/icon?family=Material+Icons",
      "https://unpkg.com/material-components-web@5.1.0/dist/material-components-web.min.css",
      "https://unpkg.com/antd@4.0.2/dist/antd.min.css"
    ]
  },
  "scripts": {
    "web": [
      "https://unpkg.com/antd@4.0.2/dist/antd.js"
    ]
  }  
}
