{
  "name": "intro",
  "routes": {
    "main": {
      "title": "Welcome",
      "icon": "home",
      "path": "/",
      "extendedMenu": [
        {
          "id": "platform",
          "title": "Platform",
          "path": "/platform"
        },
        {
          "id": "docs",
          "title": "Docs",
          "path": "/docs"
        }
      ],
      "cover": {
        "image": "castle.r.jpg",
        "opacity": 0.5,
        "navigation": true,
        "title": "Your Code Monkey Days Are Over.",
        "subtitle": "Imagine A World Beyond The Code And Start Creating.",
        "type": "default",
        "primaryActionTitle": "Get Started",
        "link": "/docs"
      },
      "components": {
        "intro": {
          "source": "summary",
          "text": "local://intro",
          "image": "logo.gif",
          "textColor": "#37474F"
        }
      }
    },
    "platform": {
      "title": "Platform",
      "path": "/platform",
      "cover": {
        "image": "fullstack.png",
        "opacity": 0.5,
        "navigation": true,
        "title": "Chunky is offering you the opportunity to implement products on multiple platforms",
        "titleStyle": {
          "textAlign": "center"
        },
        "subtitleStyle": {
          "textAlign": "center"
        },
        "subtitle": "By using Chunky you can focus more on the product itself instead of the technology stack",
        "type": "default"
      }
    },
    "webPlatform": {
      "title": "Platform",
      "path": "/platform/web",
      "cover": {
        "image": "web.svg",
        "opacity": 0.5,
        "navigation": true,
        "title": "Web Development",
        "titleStyle": {
          "textAlign": "center"
        },
        "subtitleStyle": {
          "textAlign": "center"
        },
        "subtitle": "Create beautiful Static Web Apps that you can deploy with ease to your favourite static hosting provider, such as AWS S3.",
        "type": "default"
      }
    },
    "mobilePlatform": {
      "title": "Platform",
      "path": "/platform/mobile",
      "cover": {
        "image": "mobile.svg",
        "opacity": 0.5,
        "navigation": true,
        "title": "Mobile Development",
        "titleStyle": {
          "textAlign": "center"
        },
        "subtitleStyle": {
          "textAlign": "center"
        },
        "subtitle": "Build native iOS and Android apps that you can easily package and deploy to the App Store and the Play Store.",
        "type": "default"
      }
    },
    "cloudPlatform": {
      "title": "Platform",
      "path": "/platform/cloud",
      "cover": {
        "image": "cloud.svg",
        "opacity": 0.5,
        "navigation": true,
        "title": "Cloud Development",
        "titleStyle": {
          "textAlign": "center"
        },
        "subtitleStyle": {
          "textAlign": "center"
        },
        "subtitle": "Write simple serverless cloud functions that you can deploy with a single command to AWS Lambda.",
        "type": "default"
      }
    },
    "devopsPlatform": {
      "title": "Platform",
      "path": "/platform/devops",
      "cover": {
        "image": "devops.svg",
        "opacity": 0.5,
        "navigation": true,
        "title": "Devops Development",
        "titleStyle": {
          "textAlign": "center"
        },
        "subtitleStyle": {
          "textAlign": "center"
        },
        "subtitle": "Manipulate remote data, run reports, package binaries and deploy product artifacts effortlessly from your terminal.",
        "type": "default"
      }
    },
    "docs": {
      "title": "Documentation",
      "path": "/docs",
      "variants": "sections",
      "lightThemeBackgroundColor": "#fff",
      "lightThemeTextColor": "#001528",
      "darkThemeBackgroundColor": "#37474F",
      "darkThemeTextColor": "#fff",
      "buttonsBackgroundColor": "#1E88E5",
      "buttonsTextColor": "#fff",
      "selectionBackgroundColor": "#e6f7ff",
      "cover": {
        "image": "fullstack.png",
        "opacity": 0.5,
        "navigation": true,
        "title": "Chunky Documentation",
        "type": "simple"
      }
    },
    "textExample": {
      "title": "Text Component Example",
      "path": "/docs/example/text",
      "variants": "examples",
      "components": {
        "text": {
          "source": "text",
          "textSource": "local://examples/components/text"
        }
      }
    },
    "coverExample": {
      "title": "Cover Component Example",
      "path": "/docs/example/cover",
      "variants": "examples",
      "components": {
        "text": {
          "source": "text",
          "textSource": "local://examples/components/cover"
        }
      }
    }
  }
}
